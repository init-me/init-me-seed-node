  "scripts": {
    "bootstrap": "yarn",
    "eslint": "eslint --ext=jsx,ts,tsx ./",
    "prettier": "prettier --write ./",
    "dev": "tsc",
    "d": "tsc",
    "build": "tsc",
    "w": "tsc -w",
    "prepublishOnly": "yarn prettier && yarn test",
    "test": "yarn build & jest ./test/jest/"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-yyl-node": "^0.3.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  }
}
